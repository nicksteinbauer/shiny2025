{
  "name": "shinywindows",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build",
    "sass": "sass --watch app/scss/app.scss:app/styles/app.css"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.19.3",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "remix": "^1.19.3",
    "framer-motion": "^6.5.1",
    "plaiceholder": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-text-loop-next": "^0.0.3",
    "bootstrap": "^5.3.7",
    "emailjs": "^4.0.1",
    "emailjs-com": "^3.2.0",
    "gsap": "^3.12.7",
    "lightgallery": "^2.9.0-beta.1",
    "react-bootstrap": "^2.10.9",
    "react-recaptcha": "^2.3.10",
    "react-slick": "^0.31.0",
    "reactstrap": "^9.1.6",
    "sass": "^1.85.1",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^11.7.2",
    "validator": "^13.9.0",
    "sharp": "^0.32.6",
    "react-scroll": "^1.9.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.4"
  },
  "engines": {
    "node": "18.x"
  }
}